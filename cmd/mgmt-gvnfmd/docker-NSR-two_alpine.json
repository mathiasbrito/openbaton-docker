{
  "name":"docker-test-NS-twoalpine",
  "vendor":"mcilloni",
  "version":"1.0",
  "vld":[
    {
      "name":"private"
    }
  ],
  "vnfd":[
    {
      "name":"alpine-client",
      "vendor":"mcilloni",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"CONFIGURE",
          "lifecycle_events":[
            "server_configure.sh"
          ]
        },
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh"
          ]
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "mcilloni/alpine-nop:latest"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"private"
                }
              ]
            }
          ],
          "vimInstanceName":["pop-vim-instance"]
        }
      ],
      "configurations": {
          "configurationParameters": [
              {
                  "confKey": "VAR_1",
                  "value": "testval",
                  "description": "Test value 1"
              },
              {
                  "confKey": "VAR_2",
                  "value": "22",
                  "description": "Test value 2"
              }
          ]
      },
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"docker.container"
        }
      ],
      "type":"client",
      "endpoint":"mgmt",
      "vnfPackageLocation":"https://github.com/openbaton/vnf-scripts.git"
    },
    {
      "name":"alpine-server",
      "vendor":"mcilloni",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh",
            "install-srv.sh"
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "mcilloni/alpine-nop:latest"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"private"
                }
              ]
            }
          ],
          "vimInstanceName":["pop-vim-instance"]
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"docker.container"
        }
      ],
      "type":"server",
      "endpoint":"mgmt",
      "vnfPackageLocation":"https://github.com/openbaton/vnf-scripts.git"
    }
  ],
  "vnf_dependency":[
    {
      "source":{
        "name":"alpine-server"
      },
      "target":{
        "name":"alpine-client"
      },
      "parameters":[
        "private"
      ]
    }
  ]
}