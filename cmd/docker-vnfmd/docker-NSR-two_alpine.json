{
  "name":"NSD two linked Alpine",
  "vendor":"FOKUS",
  "version":"1.0",
  "vld":[
    {
      "name":"private"
    }
  ],
  "vnfd":[
    {
      "name":"alpine-client",
      "vendor":"FOKUS",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"CONFIGURE",
          "lifecycle_events":[
            "server_configure.sh"
          ]
        },
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh"
          ]
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "alpine:latest"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"private"
                }
              ]
            }
          ],
          "vimInstanceName":["docker-pop-vim-instance"]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"docker.container"
        }
      ],
      "type":"client",
      "endpoint":"docker",
      "vnfPackageLocation":"https://github.com/openbaton/vnf-scripts.git"
    },
    {
      "name":"alpine-server",
      "vendor":"FOKUS",
      "version":"1.0",
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh",
            "install-srv.sh"
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "alpine:latest"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"private"
                }
              ]
            }
          ],
          "vimInstanceName":["docker-pop-vim-instance"]
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"docker.container"
        }
      ],
      "type":"server",
      "endpoint":"docker",
      "vnfPackageLocation":"https://github.com/openbaton/vnf-scripts.git"
    }
  ],
  "vnf_dependency":[
    {
      "source":{
        "name":"alpine-server"
      },
      "target":{
        "name":"alpine-client"
      },
      "parameters":[
        "private"
      ]
    }
  ]
}